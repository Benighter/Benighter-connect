{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/preview",
      "dest": "/public/preview.html"
    },
    {
      "src": "/room/(.*)",
      "dest": "/public/room.html"
    },
    {
      "src": "/about",
      "dest": "/public/about.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
